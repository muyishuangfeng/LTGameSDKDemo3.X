<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.gnetop.sdk.demo">

    <application
        android:name="com.gnetop.sdk.demo.MyApp"
        android:allowBackup="true"
        android:appComponentFactory="1.0.0"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:noHistory="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:appComponentFactory,android:allowBackup">
        <activity android:name="com.gnetop.sdk.demo.GuestActivity" />
        <activity android:name="com.gnetop.sdk.demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.gnetop.sdk.demo.GoogleActivity" />
        <activity android:name="com.gnetop.sdk.demo.GooglePlayActivity" />
        <activity android:name="com.gnetop.sdk.demo.FacebookActivity" />

        <activity
            android:name="com.gnetop.sdk.demo.UIActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.gnetop.sdk.demo.PortUIActivity"
            android:screenOrientation="portrait" />

        <activity android:name="com.gnetop.sdk.demo.EmailActivity" />
        <activity android:name="com.gnetop.sdk.demo.QQActivity" />
        <activity android:name="com.gnetop.sdk.demo.WeChatActivity" />

    </application>

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="com.android.vending.BILLING" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

</manifest>